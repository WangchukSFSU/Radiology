<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.radiology">
	<class name="RadiologyReportList"
		table="RadiologyReportList" >
            <id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
                <property name="studyConceptName"  column="studyConceptName" length="255" unique="true"></property>
		 <property name="htmlformuuid" column="htmlformuuid" length="255" unique="true" >
                    </property>
	</class>
</hibernate-mapping>


