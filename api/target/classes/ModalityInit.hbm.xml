<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.radiology">
	
		<class name="ModalityInit"
                table="radiology_modalityInit">
                <id name="modalityId" type="int" column="modalityId" unsaved-value="0">
                        <generator class="native" />
                </id>
                <discriminator column="modalityId" insert="false" />
                <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
                <property name="modalityName" type="java.lang.String" column="modalityName" length="255" unique="true" />
                <property name="modalityIP" type="java.lang.String" column="modalityIP" length="255" unique="true" />
                <property name="modalityPath" type="java.lang.String" column="modalityPath" length="255" /> 
        </class>
               
	
</hibernate-mapping>